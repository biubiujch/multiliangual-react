{
  "name": "multilingual-react",
  "private": false,
  "version": "0.0.11-alpha",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "react ",
    "multiliangual",
    "globalization"
  ],
  "author": "chris yang",
  "repository": "https://github.com/biubiujch/multiliangual-react.git",
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "description": "React multilingual util",
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "files": [
    "lib"
